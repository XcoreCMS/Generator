{
    "name": "xcore/generator",
    "description": "Powerful Generator built on Doctrine ORM Entity Generator & SensioGeneratorBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Vondrášek",
            "email": "pavel@appio.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Xcore\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xcore\\Generator\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~7.1",
        "doctrine/orm": "^2.5",
        "league/json-guard": "^1.0",
        "league/json-reference": "^1.0",
        "marc-mabe/php-enum": "^2.3",
        "roave/security-advisories": "dev-master",
        "symfony/validator": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "phpstan/phpstan": "^0.8.0"
    }
}
